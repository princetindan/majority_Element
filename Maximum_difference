//Implementation of maximum difference problem. 
#include <bits/stdc++.h
using namespace std;
int main(){
int n; // lenght of array
cin>>;
int a[n]; // array of lenght n
for (int i=0; i<n; i++)
{
cin>> a[i];
}
int max_dif, min_Element;
max_dif = a[1] - a[0];
min_Element = a[i];
for(int i =1; i< n; i++;)
{
if(a[i] - min_Element > max_dif)
{
max_dif = a[i] - min_Element;
}
if(a[i] < min_Element){
min_Element = a[i];
}
}
cout<< max_dif<<endl;
}
return 0;

